public class student {
	/****
	 * student class with two variables Name and age - String and int
	 */

	private String name;
	private int age;
	
	// default constructor
	public student() {
		
		
	}
	//getName method
	
	public String getName() {
	
		return name;
}

	public int getAge() {
	return age;
	
	
}

}
