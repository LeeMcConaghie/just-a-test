package uk.ac.belfastmet.examples;

import static org.junit.Assert.*;

import org.junit.Test;

public class ColorTest {

	@Test
	public void test() {
		Color color = new Color();
		String result = color.seeColor("red");
	}
	@Test
	public void test1() {
		Color color = new Color();
		String result = color.seeColor("blue");
	}
	@Test
	public void test3() {
		Color color = new Color();
		String result = color.seeColor("");
	}
}
